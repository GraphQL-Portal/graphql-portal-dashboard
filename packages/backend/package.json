{
  "name": "graphql-portal-dashboard-backend",
  "version": "0.7.0",
  "description": "Backend part of graphql-portal-dashboard",
  "author": "code.store",
  "private": true,
  "license": "MIT",
  "scripts": {
    "node-config-ts": "node-config-ts && prettier --write ./config/Config.d.ts",
    "postinstall": "yarn node-config-ts",
    "prettier": "prettier --write ./src",
    "prebuild": "rimraf dist && yarn prettier",
    "build": "nest build -p tsconfig.json",
    "start": "nest start",
    "start:debug": "node --inspect=0.0.0.0 dist/main",
    "start:prod": "node dist/src/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "clear": "rm -rf node_modules",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0' -f migrate-mongo-config.ts",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:down": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts",
    "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
